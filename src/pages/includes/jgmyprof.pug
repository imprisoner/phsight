.justified-gallery
    div.my-photo    
        a(href="./img/popgallery/image 1.jpg")
            img(src="./img/popgallery/image 1.jpg" alt="")
        .my-photo-footer.jg-caption
          .wrap
            .my-photo-preview(style="background-image: url('./img/popgallery/image 1.jpg')")
            .my-photo-actions 
                a.my-photo-edit Редактировать
                a.my-photo-replace Переместить в альбом
                a.my-photo-delete Удалить
            .my-photo-close 
                i.far.fa-times
       
